<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>demo app chat</title>
    <link rel="stylesheet" href="layout.css">
    <script src="jquery.js""></script>
    <script src=" socket.io/socket.io.js"></script>
    <script src="function.js"></script>
</head>

<body>
    <div id="wrapper">
        <div id="loginForm">
            <h3>What's your name?</h3>
            <input type="text" id="txtUsername">
            <input type="button" value="Register" id="btnRegister">
        </div>
        <div id="chatForm">
            <div id="left">
                <div id="boxTitle">User Online</div>
                <div id="boxContent">
                </div>
            </div>
            <div id="right">
                <div id="sayHi">
                    Hello <span id="currentUser">username</span>
                    <input type="button" value="Logout" id="btnLogout">
                </div>
                <div id="listMessages">

                </div>
                <div id="onTyping"></div>
                <input type="text" name="" id="txtMessage">
                <input type="button" value="Send" id="btnSendMessage">
            </div>
        </div>
    </div>

</body>

</html>