<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script src="jquery.js""></script>
    <script src=" socket.io/socket.io.js"></script>
    <script>
        // connect
        const socket = io("http://localhost/");
        // socket.on('Server-send-data', data => {
        //     $('#content').append(`${data}, `);
        // });
        // $(document).ready(() => {
        //     $('#mrA').click(() => {
        //         socket.emit('Client-send-data', 'Syhanh');
        //     });
        // });
        socket.on('receiveResultData', data => {
            $('#result').val(data);
        });
        $(document).ready(() => {
            $('#send-number').click(() => {
                const A = $('#A').val();
                const B = $('#B').val();
                socket.emit('sendNumberData', { A, B });
            });
        })
    </script>
</head>

<body>
    <!-- <div id="mrA">Click send</div>
    <h2 id="content"></h2> -->
    <div class="demoApp">
        <label for="result">Result:</label>
        <input type="number" id="result" disabled><br>
        <label for="A">A:</label>
        <input type="number" id="A"><br>
        <label for="B">B</label>
        <input type="number" id="B"><br>
        <button id="send-number">Send</button>
    </div>
</body>

</html>